	<span class="kd">public</span> <span class="nc">MemberVO</span> <span class="nf">idCheck</span><span class="o">(</span><span class="nc">MemberVO</span> <span class="n">vo</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"===&gt; MemberMapper idCheck Ìò∏Ï∂ú"</span><span class="o">);</span>
		<span class="k">return</span> <span class="o">(</span><span class="nc">MemberVO</span><span class="o">)</span> <span class="n">mybatis</span><span class="o">.</span><span class="na">selectOne</span><span class="o">(</span><span class="s">"user.idCheck"</span><span class="o">,</span> <span class="n">vo</span><span class="o">);</span>
	<span class="o">}</span>

	<span class="kd">public</span> <span class="kt">int</span> <span class="nf">memberInsert</span><span class="o">(</span><span class="nc">MemberVO</span> <span class="n">vo</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"===&gt;  MemberMapper userInsert() Ìò∏Ï∂ú"</span><span class="o">);</span>
		<span class="k">return</span> <span class="n">mybatis</span><span class="o">.</span><span class="na">insert</span><span class="o">(</span><span class="s">"user.userInsert"</span><span class="o">,</span> <span class="n">vo</span><span class="o">);</span>
	<span class="o">}</span>

