"http://mybatis.org/dtd/mybatis-3-mapper.dtd"&gt;</span>

<span class="nt">&lt;mapper</span> <span class="na">namespace=</span><span class="s">"user"</span><span class="nt">&gt;</span>
	<span class="nt">&lt;insert</span> <span class="na">id=</span><span class="s">"userInsert"</span> <span class="na">parameterType=</span><span class="s">"member"</span><span class="nt">&gt;</span>
		INSERT INTO member (
			userid,
			userpass,
			username
		) VALUES (
			#{userId},
			#{userPass},
			#{userName}
		)
	<span class="nt">&lt;/insert&gt;</span>
	<span class="c">&lt;!--  #{} : PreparedStatementÎ•º Ïù¥Ïö©ÌïòÏó¨ Î¨∏ÏûêÏó¥Ïù∏ Í≤ΩÏö∞ '' ÏûêÎèô --&gt;</span>
	<span class="c">&lt;!-- ${} : StatementÎ•º Ïù¥Ïö©ÌïòÏó¨ '' ÏûêÎèôÏúºÎ°ú Î∂ÄÏó¨ÎêòÏßÄ ÏïäÏùå (Ïª¨ÎüºÎ™ÖÏù∏ Í≤ΩÏö∞) --&gt;</span>
	
	<span class="nt">&lt;select</span> <span class="na">id=</span><span class="s">"idCheck"</span> <span class="na">parameterType=</span><span class="s">"member"</span> <span class="na">resultType=</span><span class="s">'member'</span><span class="nt">&gt;</span>
		SELECT * FROM member
		WHERE userid=#{userId} 
		<span class="nt">&lt;if</span> <span class="na">test=</span><span class="s">"userPass != null"</span><span class="nt">&gt;</span>
			AND userpass = #{userPass}
		<span class="nt">&lt;/if&gt;</span>
	<span class="nt">&lt;/select&gt;</span>
