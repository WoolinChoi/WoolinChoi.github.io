<span class="nc">.email</span> <span class="p">{</span><span class="nl">cursor</span><span class="p">:</span> <span class="nb">crosshair</span><span class="p">;}</span>
<span class="nc">.copyright</span> <span class="p">{</span><span class="nl">cursor</span><span class="p">:</span> <span class="nb">wait</span><span class="p">;}</span>
<span class="nt">&lt;/style&gt;</span>
<span class="nt">&lt;/head&gt;</span>
<span class="nt">&lt;body&gt;</span>
<span class="nt">&lt;div</span> <span class="na">id=</span><span class="s">"footer"</span><span class="nt">&gt;</span>
  <span class="nt">&lt;div</span> <span class="na">class=</span><span class="s">"footerInner"</span><span class="nt">&gt;</span> <span class="c">&lt;!-- 전체를 할 땐 --&gt;</span>
    <span class="nt">&lt;p</span> <span class="na">class=</span><span class="s">"footerLogo"</span><span class="nt">&gt;&lt;img</span> <span class="na">src=</span><span class="s">"../images/common/logo_copyright.gif"</span> <span class="na">alt=</span><span class="s">"WebCafe"</span> <span class="nt">/&gt;&lt;/p&gt;</span>
    <span class="nt">&lt;h2&gt;</span>사이트 이용안내<span class="nt">&lt;/h2&gt;</span>
