	<span class="nt">tr</span><span class="nd">:nth-child</span><span class="o">(</span><span class="nt">odd</span><span class="o">)</span> <span class="p">{</span><span class="nl">background</span><span class="p">:</span> <span class="no">olive</span><span class="p">;}</span>
	<span class="nt">tr</span><span class="nd">:hover</span> <span class="p">{</span><span class="nl">background</span><span class="p">:</span> <span class="no">pink</span><span class="p">;}</span>
