<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="err">$</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
	<span class="err">$</span><span class="o">(</span><span class="err">'</span><span class="o">.</span><span class="na">accordion</span><span class="err">'</span><span class="o">).</span><span class="na">each</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
		<span class="kt">var</span> <span class="n">allDt</span> <span class="o">=</span> <span class="err">$</span><span class="o">(</span><span class="k">this</span><span class="o">).</span><span class="na">find</span><span class="o">(</span><span class="err">'</span><span class="n">dt</span><span class="err">'</span><span class="o">);</span>
		<span class="kt">var</span> <span class="n">allDd</span> <span class="o">=</span> <span class="err">$</span><span class="o">(</span><span class="k">this</span><span class="o">).</span><span class="na">find</span><span class="o">(</span><span class="err">'</span><span class="n">dd</span><span class="err">'</span><span class="o">);</span>
		
		<span class="n">allDd</span><span class="o">.</span><span class="na">hide</span><span class="o">();</span>
		<span class="n">allDt</span><span class="o">.</span><span class="na">css</span><span class="o">(</span><span class="err">'</span><span class="n">cursor</span><span class="err">'</span><span class="o">,</span> <span class="err">'</span><span class="n">pointer</span><span class="err">'</span><span class="o">);</span>
		
		<span class="n">allDt</span><span class="o">.</span><span class="na">click</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
